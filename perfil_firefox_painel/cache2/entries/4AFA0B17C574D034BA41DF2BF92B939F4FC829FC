    if (typeof element != "undefined" && element != null)
        scrolla = true;

    if (scrolla) {
        element.scrollIntoView({
            behavior: 'smooth'
        });
        //document.getElementById("teste").innerHTML = elementDemo.innerHTML + " => scrollou";
    };
